{
    "name": "jinseokoh/bootpay",
    "description": "Unofficial BootPay package for Laravel",
    "type": "library",
    "version": "1.2.0",
    "keywords": ["Laravel", "BootPay"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chuck JS. Oh",
            "email": "jinseokoh@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "illuminate/support": "~5.6 || ~6.0 || ~7.0 || ~8.0 || ~9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "JinseokOh\\BootPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JinseokOh\\BootPay\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JinseokOh\\BootPay\\BootPayServiceProvider"
            ],
            "aliases": {
                "BootPay": "JinseokOh\\BootPay\\BootPayFacade"
            }
        }
    }
}
